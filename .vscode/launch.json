{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Extension Tests",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"${workspaceFolder}",
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--extensionTestsPath=${workspaceFolder}/test"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"preLaunchTask": "npm: compile",
			"postDebugTask": "npm: clean",
			"timeout": 60000
		}
	]
}